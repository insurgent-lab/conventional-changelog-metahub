{
  "name": "@insurgent/conventional-changelog-preset",
  "description": "conventional-changelog preset",
  "version": "8.0.0",
  "author": "Pierre Vanduynslager",
  "maintainers": [
    "Pierre Cavin <me@sherlox.io>"
  ],
  "license": "MIT",
  "exports": {
    ".": "./index.js",
    "./release-rules": "./release-rules.js"
  },
  "type": "module",
  "scripts": {
    "lint:eslint": "eslint . --ext .js",
    "lint:prettier": "prettier ./**/*.{json,md,yml} --check",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
    "test": "cross-env NODE_OPTIONS=\"--loader=esmock --no-warnings\" c8 ava --no-worker-threads -v",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "conventional-changelog-conventionalcommits": "^7.0.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "ava": "5.3.1",
    "c8": "^8.0.1",
    "conventional-changelog": "5.1.0",
    "cross-env": "^7.0.3",
    "emoji-regex": "10.3.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-ava": "14.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "esmock": "2.5.9",
    "execa": "5.1.1",
    "fs-extra": "11.1.1",
    "get-stream": "6.0.1",
    "p-each-series": "2.2.0",
    "prettier": "3.0.3",
    "proxyquire": "2.1.3",
    "semantic-release": "22.0.6",
    "stringz": "2.1.0",
    "tempy": "1.0.1"
  },
  "peerDependencies": {
    "@semantic-release/commit-analyzer": ">= 11.1.0",
    "@semantic-release/release-notes-generator": ">= 12.1.0"
  },
  "peerDependenciesMeta": {
    "@semantic-release/release-notes-generator": {
      "optional": true
    },
    "@semantic-release/commit-analyzer": {
      "optional": true
    }
  },
  "homepage": "https://github.com/insurgent-lab/conventional-changelog-preset#readme",
  "bugs": {
    "url": "https://github.com/insurgent-lab/conventional-changelog-preset/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insurgent-lab/conventional-changelog-preset.git"
  },
  "keywords": [
    "changelog",
    "conventional-changelog",
    "conventional-commits",
    "publish",
    "release",
    "semantic-release"
  ],
  "files": [
    "lib/*.js",
    "index.js",
    "release-rules.js",
    "types.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "15s"
  },
  "c8": {
    "include": [
      "lib/*.js",
      "index.js",
      "release-rules.js",
      "types.js"
    ],
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
