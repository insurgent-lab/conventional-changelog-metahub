{
  "name": "@insurgentlab/conventional-changelog-preset",
  "description": "conventional-changelog preset",
  "version": "6.0.4",
  "author": "Pierre Vanduynslager",
  "maintainers": [
    "Pierre Cavin <me@sherlox.io>"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint:eslint": "eslint . --ext .js",
    "lint:prettier": "prettier ./**/*.{json,md,yml} --check",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
    "release": "semantic-release",
    "test": "nyc ava --no-worker-threads -v"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "10.0.5",
    "@semantic-release/release-notes-generator": "11.0.6",
    "ava": "5.3.1",
    "conventional-changelog": "4.0.0",
    "emoji-regex": "10.2.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-ava": "14.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "execa": "5.1.1",
    "fs-extra": "11.1.1",
    "get-stream": "6.0.1",
    "nyc": "15.1.0",
    "p-each-series": "2.2.0",
    "prettier": "3.0.2",
    "proxyquire": "2.1.3",
    "semantic-release": "21.1.1",
    "stringz": "2.1.0",
    "tempy": "1.0.1"
  },
  "homepage": "https://github.com/insurgent-lab/conventional-changelog-preset#readme",
  "bugs": {
    "url": "https://github.com/insurgent-lab/conventional-changelog-preset/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insurgent-lab/conventional-changelog-preset.git"
  },
  "keywords": [
    "changelog",
    "conventional-changelog",
    "conventional-commits",
    "publish",
    "release",
    "semantic-release"
  ],
  "files": [
    "lib/*.js",
    "index.js",
    "release-rules.js",
    "types.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "15s"
  },
  "nyc": {
    "include": [
      "lib/*.js",
      "index.js",
      "release-rules.js",
      "types.js"
    ],
    "reporter": [
      "html",
      "json",
      "text"
    ],
    "all": true
  }
}
